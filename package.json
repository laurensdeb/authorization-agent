{
  "name": "@laurensdeb/authorization-agent",
  "private": true,
  "scripts": {
    "bootstrap": "npm ci && lerna bootstrap",
    "clean": "npm run clean:modules",
    "clean:modules": "lerna clean && shx rm -rf ./node_modules/",
    "lint:all": "lerna run lint --parallel",
    "lint:fix:all": "lerna run lint:fix --parallel",
    "test:all": "lerna run test --parallel",
    "build:all": "lerna run build",
    "start": "lerna run start --parallel",
    "commit": "commit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/config-lerna-scopes": "^16.0.0",
    "@commitlint/prompt-cli": "^16.1.0",
    "@types/jest": "^27.4.1",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "shx": "^0.3.3"
  },
  "dependencies": {
  }
}
