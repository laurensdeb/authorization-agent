{
    "@context": [
        "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-uma/^1.0.0/components/context.jsonld",
        "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-interop/^1.0.0/components/context.jsonld",
        "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-helpers/^1.0.0/components/context.jsonld"
    ],
    "@graph": [
        {
            "@id": "urn:authorization-service:default:AuthorizationAgentFactory",
