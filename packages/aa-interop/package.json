{
    "name": "@laurensdeb/authorization-agent-interop",
    "version": "1.0.0",
    "author": "Laurens Debackere <Laurens.Debackere@UGent.be> (https://thundr.be)",
    "license": "AGPL-3.0-only",
    "private": true,
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "require": "./dist/index.js"
      }
    },
    "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-interop",
    "lsd:components": "dist/components/components.jsonld",
    "lsd:contexts": {
      "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-interop/^1.0.0/components/context.jsonld": "dist/components/context.jsonld"
    },
    "lsd:importPaths": {
      "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-interop/^1.0.0/components/": "dist/components/",
      "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-interop/^1.0.0/config/": "config/",
      "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-interop/^1.0.0/dist/": "dist/"
    },
    "scripts": {
      "build": "npm run build:ts && npm run build:components",
      "build:components": "componentsjs-generator -s lib -c dist/components -i .componentsignore --lenient",
      "prepare": "npm run build",
      "build:ts": "tsc",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix",
      "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
      "@tsconfig/node16": "^1.0.2",
      "@typescript-eslint/eslint-plugin": "^5.12.1",
      "@typescript-eslint/parser": "^5.12.1",
      "componentsjs-generator": "^2.6.1",
      "eslint": "^8.10.0",
      "eslint-config-google": "^0.14.0",
      "typescript": "^4.5.5"
    },
    "dependencies": {
      "componentsjs": "^4.5.0",
      "@digita-ai/handlersjs-http": "0.7.1"
    },
    "files": [
        ".componentsignore",
        "dist",
        "config"
      ]
}
  