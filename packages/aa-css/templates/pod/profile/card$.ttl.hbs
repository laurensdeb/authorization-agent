@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix interop: <http://www.w3.org/ns/solid/interop#>.
@prefix solid: <http://www.w3.org/ns/solid/terms#>.

<>
    a foaf:PersonalProfileDocument;
    foaf:maker <{{webId}}>;
    foaf:primaryTopic <{{webId}}>.

<{{webId}}>
    {{#if name}}foaf:name "{{name}}";{{/if}}
    {{#if oidcIssuer}}solid:oidcIssuer <{{oidcIssuer}}>;{{/if}}
    interop:hasRegistrySet <{{podBaseUrl}}registries/index>;
    interop:hasAuthorizationAgent <http://localhost:5000>;
    interop:hasInbox <{{podBaseUrl}}inbox/general> ;
    interop:hasAccessInbox <{{podBaseUrl}}inbox/access> ;
    a foaf:Person, interop:Agent.
